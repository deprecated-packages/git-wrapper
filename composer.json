{
    "name": "symplify/git-wrapper",
    "description": "A PHP wrapper around the Git command line utility.",
    "license": "MIT",
    "keywords": ["git", "git wrapper", "cli"],
    "require": {
        "php": ">=8.0",
        "symfony/process": "^5.3",
        "symfony/event-dispatcher": "^5.3",
        "nette/utils": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/smart-file-system": "^9.4.1",
        "psr/log": "^1.1",
        "ondram/ci-detector": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\GitWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\GitWrapper\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
